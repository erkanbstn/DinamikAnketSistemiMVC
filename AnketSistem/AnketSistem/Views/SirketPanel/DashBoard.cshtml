
@{
    ViewBag.Title = "DashBoard";
}

<div class="row state-overview">
    <div class="col-lg-3 col-sm-6">
        <section class="panel">
            <div class="symbol terques">
                <i class="fa fa-user"></i>
            </div>
            <div class="value">
                <h1 >
                    @ViewBag.personel
                </h1>
                <p>Toplam Personel</p>
            </div>
        </section>
    </div>
    <div class="col-lg-3 col-sm-6">
        <section class="panel">
            <div class="symbol red">
                <i class="fa fa-tags"></i>
            </div>
            <div class="value">
                <h1>
                    @ViewBag.anket
                </h1>
                <p>Toplam Anket</p>
            </div>
        </section>
    </div>
    <div class="col-lg-3 col-sm-6">
        <section class="panel">
            <div class="symbol yellow">
                <i class="fa fa-shopping-cart"></i>
            </div>
            <div class="value">
                <h1>
                    @ViewBag.soru
                </h1>
                <p>Toplam Soru</p>
            </div>
        </section>
    </div>
    <div class="col-lg-3 col-sm-6">
        <section class="panel">
            <div class="symbol blue">
                <i class="fa fa-bar-chart-o"></i>
            </div>
            <div class="value">
                <h1 >
                    @ViewBag.yorum
                </h1>
                <p>Toplam Yorum</p>
            </div>
        </section>
    </div>
</div>
