
@{
    ViewBag.Title = "Personeller";
}

<table class="table table-hover">
    <tbody>
        <tr>
            <th>
                Personel
            </th>
            <th>
                Düzenle
            </th>
         
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.PersonelAd
                </td>
                <td>
                    <a class="btn btn-success" href="~/SirketPanel/PersonelDuzenle/@item.PersonelID">Düzenle</a>
                </td>
            </tr>
        }
    </tbody>
</table>

